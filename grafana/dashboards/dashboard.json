{
  "id": null,
  "uid": "mobile-shop-microservices", 
  "title": "Mobile Shop - NestJS Microservices Dashboard",
  "description": "Comprehensive monitoring dashboard for Mobile Shop microservices architecture",
  "tags": ["nestjs", "microservices", "prometheus", "mobile-shop"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "System Overview",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Services Status",
      "description": "Number of healthy microservices",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(up{job=~\"$service\"})",
          "refId": "healthy_services",
          "legendFormat": "Healthy Services"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 6 },
              { "color": "green", "value": 8 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Total Request Rate",
      "description": "Requests per second across all services",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=~\"$service\"}[5m]))",
          "refId": "total_request_rate",
          "legendFormat": "Total Requests/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Avg Response Time",
      "description": "Average response time across all services",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "avg(rate(http_request_duration_seconds_sum{job=~\"$service\"}[5m]) / rate(http_request_duration_seconds_count{job=~\"$service\"}[5m]))",
          "refId": "avg_response_time",
          "legendFormat": "Average Response Time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Error Rate",
      "description": "5xx error rate percentage",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=~\"$service\",status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=~\"$service\"}[5m])) * 100",
          "refId": "error_rate",
          "legendFormat": "Error Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Service Health Status",
      "description": "Uptime status for each microservice",
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "up{job=~\"$service\"}",
          "refId": "service_health",
          "legendFormat": "{{instance}} - {{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter",
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": { "mode": "none" }
          },
          "unit": "short",
          "min": 0,
          "max": 1
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 7,
      "type": "row",
      "title": "Request Metrics",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Request Rate by Service",
      "description": "HTTP request rate for each microservice",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=~\"$service\"}[5m])) by (job)",
          "refId": "request_rate_by_service",
          "legendFormat": "{{job}} Requests/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "stacking": { "mode": "none" }
          },
          "color": { "mode": "palette-classic" }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "lastNotNull"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Response Time Percentiles",
      "description": "Response time distribution",
      "gridPos": { "h": 6, "w": 16, "x": 8, "y": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\"}[5m])) by (le))",
          "refId": "p95_response_time",
          "legendFormat": "95th Percentile (P95)"
        },
        {
          "expr": "histogram_quantile(0.75, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\"}[5m])) by (le))",
          "refId": "p75_response_time",
          "legendFormat": "75th Percentile (P75)"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\"}[5m])) by (le))",
          "refId": "median_response_time",
          "legendFormat": "Median (P50)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 0
          }
        }
      }
    },
    {
      "id": 10,
      "type": "row",
      "title": "API Gateway Metrics",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 12 }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "API Gateway - Request Rate by Endpoint",
      "description": "Request rate for each API endpoint",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 13 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"api-gateway\"}[5m])) by (route, method)",
          "refId": "api_gateway_endpoints",
          "legendFormat": "{{method}} {{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 1,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "lastNotNull"] }
      }
    },
    {
      "id": 12,
      "title": "Error Rate by Service",
      "type": "timeseries",
      "description": "5xx error rate percentage per service",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 13 },
      "targets": [
        {
          "expr": "sum by (job) (rate(http_request_duration_seconds_count{job=~\"$service\", status=~\"5..\"}[5m])) / sum by (job) (rate(http_request_duration_seconds_count{job=~\"$service\"}[5m])) * 100",
          "refId": "service_error_rates",
          "legendFormat": "{{job}} Error Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "id": 13,
      "type": "row",
      "title": "Database & Infrastructure",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 19 }
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "CPU Usage by Service",
      "description": "CPU utilization percentage",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job=~\"$service\"}[5m]) * 100",
          "refId": "cpu_usage_by_service",
          "legendFormat": "{{job}} CPU Usage"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 15,
      "type": "timeseries",
      "title": "Memory Usage",
      "description": "Memory usage in MB",
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 20 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job=~\"$service\"} / 1024 / 1024",
          "refId": "memory_usage_by_service",
          "legendFormat": "{{job}} Memory (MB)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "MB",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 16,
      "type": "stat",
      "title": "RabbitMQ Queue Messages",
      "description": "Total messages in RabbitMQ queues",
      "gridPos": { "h": 3, "w": 4, "x": 16, "y": 20 },
      "targets": [
        {
          "expr": "sum(rabbitmq_queue_messages)",
          "refId": "rabbitmq_messages",
          "legendFormat": "Total Messages"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 1000 }
            ]
          }
        }
      }
    },
    {
      "id": 17,
      "type": "stat",
      "title": "PostgreSQL Connections",
      "description": "Active database connections",
      "gridPos": { "h": 3, "w": 4, "x": 20, "y": 20 },
      "targets": [
        {
          "expr": "pg_stat_database_numbackends{datname!=\"template0\",datname!=\"template1\",datname!=\"postgres\"}",
          "refId": "postgres_connections",
          "legendFormat": "{{datname}} Connections"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      }
    },
    {
      "id": 26,
      "type": "stat", 
      "title": "Prometheus TSDB Size",
      "description": "Size of Prometheus time series database",
      "gridPos": { "h": 3, "w": 4, "x": 16, "y": 23 },
      "targets": [
        {
          "expr": "prometheus_tsdb_size_bytes / 1024 / 1024",
          "refId": "prometheus_tsdb_size",
          "legendFormat": "TSDB Size (MB)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "MB",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area", 
        "justifyMode": "center",
        "orientation": "horizontal"
      }
    },
    {
      "id": 27,
      "type": "stat",
      "title": "Active Series",
      "description": "Number of active time series",
      "gridPos": { "h": 3, "w": 4, "x": 20, "y": 23 },
      "targets": [
        {
          "expr": "prometheus_tsdb_head_series",
          "refId": "prometheus_active_series",
          "legendFormat": "Active Time Series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100000 },
              { "color": "red", "value": 500000 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center", 
        "orientation": "horizontal"
      }
    },
    {
      "id": 18,
      "type": "row",
      "title": "Business Metrics",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 }
    },
    {
      "id": 19,
      "type": "stat",
      "title": "Orders Per Minute",
      "description": "Order creation rate",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 28 },
      "targets": [
        {
          "expr": "rate(orders_created_total[5m]) * 60",
          "refId": "orders_per_minute",
          "legendFormat": "Orders Created/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        }
      }
    },
    {
      "id": 20,
      "type": "stat",
      "title": "Payment Success Rate",
      "description": "Successful payment percentage",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 28 },
      "targets": [
        {
          "expr": "sum(rate(payments_completed_total[5m])) / sum(rate(payments_total[5m])) * 100",
          "refId": "payment_success_rate",
          "legendFormat": "Payment Success %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 90 },
              { "color": "green", "value": 95 }
            ]
          }
        }
      }
    },
    {
      "id": 21,
      "type": "timeseries",
      "title": "Circuit Breaker Status",
      "description": "Circuit breaker state for each service",
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 28 },
      "targets": [
        {
          "expr": "circuit_breaker_state",
          "refId": "circuit_breaker_status",
          "legendFormat": "{{service}} â†’ {{target_service}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter",
            "lineWidth": 2
          },
          "mappings": [
            { "options": { "0": { "text": "CLOSED", "color": "green" } } },
            { "options": { "1": { "text": "OPEN", "color": "red" } } },
            { "options": { "2": { "text": "HALF_OPEN", "color": "yellow" } } }
          ]
        }
      }
    },
    {
      "id": 22,
      "type": "row",
      "title": "AI Service Metrics",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 }
    },
    {
      "id": 23,
      "type": "timeseries",
      "title": "AI Requests & Response Times",
      "description": "AI service request rate and response times",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 33 },
      "targets": [
        {
          "expr": "rate(ai_requests_total[5m])",
          "refId": "ai_request_rate",
          "legendFormat": "AI Request Rate"
        },
        {
          "expr": "histogram_quantile(0.95, rate(ai_request_duration_seconds_bucket[5m]))",
          "refId": "ai_response_time_p95",
          "legendFormat": "AI Response Time (P95)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "AI Request Rate" },
            "properties": [{ "id": "unit", "value": "reqps" }]
          },
          {
            "matcher": { "id": "byName", "options": "AI Response Time (P95)" },
            "properties": [{ "id": "unit", "value": "s" }]
          }
        ]
      }
    },
    {
      "id": 24,
      "type": "stat",
      "title": "Qdrant Vector DB Status",
      "description": "Vector database collection count",
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 33 },
      "targets": [
        {
          "expr": "qdrant_collections_total",
          "refId": "qdrant_collections",
          "legendFormat": "Vector Collections"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "id": 25,
      "type": "table",
      "title": "Top API Endpoints by Request Count",
      "description": "Most frequently called API endpoints",
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 33 },
      "targets": [
        {
          "expr": "topk(10, sum(rate(http_requests_total{job=\"api-gateway\"}[5m])) by (route, method))",
          "refId": "top_api_endpoints",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "auto"
          }
        }
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "Value": "Requests/sec",
              "route": "Route",
              "method": "Method"
            }
          }
        }
      ]
    }
  ],
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "style": "dark"
}