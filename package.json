{
  "name": "mobile-shop",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:generate:user": "npx prisma generate --schema=./prisma/user/schema.prisma",
    "prisma:generate:phone": "npx prisma generate --schema=./prisma/phone/schema.prisma",
    "prisma:generate:order": "npx prisma generate --schema=./prisma/order/schema.prisma",
    "prisma:generate:all": "npm run prisma:generate:user && npm run prisma:generate:phone && npm run prisma:generate:order",
    "prisma:migrate:user": "npx prisma migrate deploy --schema=./prisma/user/schema.prisma",
    "prisma:migrate:phone": "npx prisma migrate deploy --schema=./prisma/phone/schema.prisma",
    "prisma:migrate:order": "npx prisma migrate deploy --schema=./prisma/order/schema.prisma",
    "prisma:migrate:all": "npm run prisma:migrate:user && npm run prisma:migrate:phone && npm run prisma:migrate:order",
    "build": "nest build --builder webpack",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/mobile-shop/main",
    "start:api-gateway": "nest start api-gateway",
    "start:auth-service": "nest start auth-service",
    "start:user-service": "nest start user-service",
    "start:phone-service": "nest start phone-service",
    "start:order-service": "nest start order-service",
    "start:ai-service": "nest start ai-service",
    "start:dev:api-gateway": "nest start api-gateway --watch",
    "start:dev:auth-service": "nest start auth-service --watch",
    "start:dev:user-service": "nest start user-service --watch",
    "start:dev:phone-service": "nest start phone-service --watch",
    "start:dev:order-service": "nest start order-service --watch",
    "start:dev:ai-service": "nest start ai-service --watch",
    "start:all": "concurrently --names \"API,AUTH,USER,PHONE,ORDER,AI\" --prefix-colors \"green,blue,yellow,red,magenta,cyan\" \"npm run start:api-gateway\" \"npm run start:auth-service\" \"npm run start:user-service\" \"npm run start:phone-service\" \"npm run start:order-service\" \"npm run start:ai-service\"",
    "start:dev:all": "concurrently --names \"API,AUTH,USER,PHONE,ORDER,AI\" --prefix-colors \"green,blue,yellow,red,magenta,cyan\" \"npm run start:dev:api-gateway\" \"npm run start:dev:auth-service\" \"npm run start:dev:user-service\" \"npm run start:dev:phone-service\" \"npm run start:dev:order-service\" \"npm run start:dev:ai-service\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:api-gateway": "jest --testPathPatterns=apps/api-gateway",
    "test:auth-service": "jest --testPathPatterns=apps/auth-service",
    "test:user-service": "jest --testPathPatterns=apps/user-service",
    "test:phone-service": "jest --testPathPatterns=apps/phone-service",
    "test:order-service": "jest --testPathPatterns=apps/order-service",
    "test:ai-service": "jest --testPathPatterns=apps/ai-service",
    "test:contracts": "jest --testPathPatterns=libs/contracts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/mobile-shop/test/jest-e2e.json"
  },
  "dependencies": {
    "@langchain/core": "^0.3.78",
    "@langchain/openai": "^0.6.14",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.16.3",
    "@qdrant/js-client-rest": "^1.15.1",
    "@types/axios": "^0.9.36",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.9",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.35",
    "opossum": "^9.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^8.3.2",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/opossum": "^8.1.9",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.16.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/contracts(|/.*)$": "<rootDir>/libs/contracts/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid)/)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "transpilation": true
      }
    }
  }
}
